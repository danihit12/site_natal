FROM blog_post
ORDER BY created_at DESC;</sql><sql name="SQL 2*">SELECT *
FROM blog_comment
WHERE post_id = 5;
</sql><sql name="SQL 3*">SELECT 
    c.id,
    c.author,
    c.text,
    c.created_at,
    p.title AS post_title,
    p.created_at AS post_date
FROM blog_comment AS c
JOIN blog_post AS p ON c.post_id = p.id
WHERE c.post_id = 5;
</sql><sql name="SQL 4*">SELECT
    cat.id AS category_id,
    cat.name AS category_name,
    p.id AS post_id,
    p.title AS post_title,
    p.created_at
FROM blog_category AS cat
JOIN blog_post_categories AS pc ON pc.category_id = cat.id
JOIN blog_post AS p ON p.id = pc.post_id
WHERE cat.id = 2;

</sql><sql name="SQL 5*">SELECT 
    c.id AS category_id,
    c.name AS category_name,
    COUNT(pc.post_id) AS total_posts
FROM blog_category AS c
JOIN blog_post_categories AS pc 
    ON pc.category_id = c.id
JOIN blog_post AS p
    ON p.id = pc.post_id
GROUP BY c.id, c.name
HAVING COUNT(pc.post_id) &gt;= 2;
</sql><current_tab id="4"/></tab_sql></sqlb_project>
